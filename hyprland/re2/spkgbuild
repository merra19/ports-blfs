# description	: An efficient, principled regular expression library
# depends	: abseil-cpp gcc

name=re2
version=20240702
_re2ver=2024-07-02
release=3
source="$name-$version.tar.gz::https://github.com/google/re2/archive/$_re2ver.tar.gz"

build() {
	cd $name-$_re2ver

    make 
}

package() {
    cd $name-$_re2ver

    make prefix=/usr DESTDIR="$PKG" install
}